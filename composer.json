{
    "name": "raindrop/twigloader-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Raindrop TwigLoaderBundle",
    "keywords": ["twig", "loader", "database", "doctrine"],
    "homepage": "https://github.com/raindropdevs",
    "license": "MIT",
    "authors": [
        {
            "name": "Raindrop Dev Team",
            "homepage": "https://github.com/raindropdevs/RaindropTwigLoaderBundle/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.1.0,<2.3",
        "symfony/yaml": "2.1.*@dev",
        "symfony/monolog-bundle": ">=2.1,<3",
        "doctrine/doctrine-bundle": "1.1.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "twig/twig": ">=1.6.0,<2.0",
        "sonata-project/doctrine-extensions": "1.*",
        "raindrop/syntax-highlight-bundle": "dev-master"
    },
    "require-dev": {
        "symfony/class-loader": "2.1.x@dev",
        "symfony/form": "2.1.x@dev",
        "symfony/finder": "2.1.x@dev",
        "symfony/twig-bundle": "2.1.x@dev",
        "symfony/twig-bridge": "2.1.x@dev"
    },
    "suggests": {
        "sonata-project/admin-bundle": "Sonata Admin support"
    },
    "autoload": {
        "psr-0": { "Raindrop\\TwigLoaderBundle": "" }
    },
    "target-dir": "Raindrop/TwigLoaderBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
